{
  "name": "@diabeticum-pedis/front-end",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "start": "npx vite",
    "build": "npx vite build",
    "docker:build": "docker build -f .docker/Dockerfile -t diabeticum-pedis/front-end:latest .",
    "docker-compose:build": "docker compose -f .docker/compose.yml build",
    "docker-compose:up": "docker compose -f .docker/compose.yml up -d",
    "docker-compose:down": "docker compose -f .docker/compose.yml down",
    "test": "npx vitest run",
    "test:dev": "npx vitest",
    "lint": "npx eslint \"**/*\"",
    "format:check": "npx prettier -l .",
    "format:write": "npx prettier -w ."
  },
  "dependencies": {
    "bootstrap": "~5.3.0",
    "class-transformer": "^0.5.1",
    "react-router-dom": "~7.9.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/js": "~9.39.0",
    "@types/node": "~24.10.0",
    "@types/react": "~19.2.0",
    "@types/react-dom": "~19.2.0",
    "@vitejs/plugin-react": "~5.1.0",
    "@vitest/browser-playwright": "~4.0.0",
    "@vitest/coverage-v8": "~4.0.0",
    "@vitest/ui": "~4.0.0",
    "eslint": "~9.39.0",
    "eslint-config-prettier": "~10.1.0",
    "eslint-plugin-react-hooks": "~7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "~16.5.0",
    "playwright": "~1.57.0",
    "prettier": "~3.7.0",
    "prettier-plugin-organize-imports": "~4.3.0",
    "react": "~19.2.0",
    "react-dom": "~19.2.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "~8.48.0",
    "vite": "~7.2.0",
    "vitest": "~4.0.0",
    "vitest-browser-react": "~2.0.0"
  }
}
